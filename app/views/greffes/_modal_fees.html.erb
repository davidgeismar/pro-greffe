<div class="modal fade lg-modal-agency-fees" tabindex="-1" role="dialog"  aria-hidden="true">
  <div class="modal-dialog xl-modal" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-center">Voici la liste des frais bancaires que vous payez actuellement avec <span class='extra-strong'><%= greffe.name%></span> </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%# render partial: "greffes/presentation/fees_table", locals:{greffe: greffe} %>
      </div>
      <div class="modal-footer">
        <%= render partial: "greffes/presentation/call_to_actions", locals:{greffe: greffe} %>
      </div>
    </div>
  </div>
</div>
