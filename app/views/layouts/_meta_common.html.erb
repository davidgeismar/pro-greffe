<meta itemprop="image" content="<%= content_for?(:image) ? yield(:image) : "https://www.Greffe-sans-frais.com/social-logo.png" %>">

<!-- Twitter Card data -->
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="@GreffeSansFrais">
<meta name="twitter:title" content="<%= content_for?(:title) ? yield(:title) : "Frais des Greffes - Pro-greffe" %>">
