<!-- Schema.org markup for Google+ -->
<meta itemprop="name" content="<%= content_for?(:title) ? yield(:title) : "Frais des Greffes - Greffe Sans Frais" %>">
<meta itemprop="description" content="<%= content_for?(:description) ? yield(:description) : "Les frais bancaires de toutes les Greffes. Réduire ses frais bancaires facilement. Négociez les frais de votre Greffe. Comparer les frais. Changer de Greffe pour une Greffe sans frais"%>">

<meta name="keywords" lang="fr" content="<%= content_for?(:keywords) ? yield(:keywords) : "frais bancaires, economiser ses frais bancaires, négociez frais bancaires" %>">
<meta itemprop="image" content="<%= content_for?(:image) ? yield(:image) : "https://www.Greffe-sans-frais.com/social-logo.png" %>">

<!-- Twitter Card data -->
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="@GreffeSansFrais">
<meta name="twitter:title" content="<%= content_for?(:title) ? yield(:title) : "Frais des Greffes - Greffe Sans Frais" %>">
