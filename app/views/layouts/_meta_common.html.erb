<meta name="twitter:url" content="<%= "https://#{request.host+request.fullpath}" %>">

<!-- Open Graph data -->
<meta property="og:title" content="<%= content_for?(:title) ? yield(:title) : "Frais des Greffes - Pro-greffe" %>" />
<meta property="og:type" content="website" />
