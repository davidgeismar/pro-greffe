
<div class="row">
		<div class="text-center pagination-hd">
			<%= will_paginate greffes, renderer: WillPaginate::ActionView::BootstrapLinkRenderer  %>
		</div>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Titre</th>
			<th>Phone</th>
			<th>URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% greffes.each do |greffe| %>
      <tr>
        <td><%= link_to greffe.name, administration_greffe_path(greffe) %></td>
				<td><%= greffe.phone %></td>
				<td><%= greffe.website_url %></td>
        <td><%= link_to 'Voir', administration_greffe_path(greffe) %></td>
        <td><%= link_to 'Modifier', edit_administration_greffe_path(greffe) %></td>
        <td><%= link_to 'Supprimer', administration_greffe_path(greffe), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>



<div class="row">
		<div class="text-center pagination-hd">
			<%= will_paginate greffes, renderer: WillPaginate::ActionView::BootstrapLinkRenderer  %>
		</div>
</div>
